{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProducerConfig": {
    "BootstrapServers": "localhost:9092" //"kafka:29092"
  },
  "ConsumerConfig": {
    "GroupId": "gateway-group2",
    "BootstrapServers": "localhost:9092", //"kafka:29092",
    "AutoOffsetReset": 1 //"AutoOffsetReset.Earliest"
  },
  "RedisConfig": {
    "Configuration": "localhost:6379",//"eda_redis:6379",
    "DefaultExpiryMin": 30,
    "DefaultTimeoutMin": 5
  }


}
